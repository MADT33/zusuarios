          <Input id="inputApellido" value="ID" width="4rem"/>
           <Input id="inputEmail" value="ID" width="12rem"/>
  </FlexBox>



      <FlexBox justifyContent="SpaceBetween" alignItems="Center" backgroundDesign="Transparent">
					<Button width="12em" text="Cancel" class="sapUiMediumMarginBegin" press="oncancelar" type="Emphasized"></Button>
					<Button width="12em" text="Uplaad" class="sapUiMediumMarginBegin" press="onUploadDatos" type="Emphasized"></Button>
				</FlexBox>
